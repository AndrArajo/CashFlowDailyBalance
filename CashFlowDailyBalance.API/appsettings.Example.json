{
  "ConnectionStrings": {
    "DefaultConnection": "Host={DB_HOST};Port={DB_PORT};Database={POSTGRES_DB};Username={POSTGRES_USER};Password={POSTGRES_PASSWORD}",
    "TransactionApiUrl": "{TRANSACTION_API_URL}"
  },
  "Logging": {
    "LogLevel": {
      "Default": "{LOG_LEVEL}",
      "Microsoft.AspNetCore": "Warning",
      "Microsoft.EntityFrameworkCore": "Debug"
    }
  },
  "AllowedHosts": "*"
} 